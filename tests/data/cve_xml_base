    <vuln:published-datetime>2017-03-16T20:59:02.510-04:00</vuln:published-datetime>
    <vuln:last-modified-datetime>2017-03-20T09:26:49.150-04:00</vuln:last-modified-datetime>
    <vuln:cvss>
      <cvss:base_metrics>
