    <vuln:last-modified-datetime>2017-05-08T13:29:00.220-04:00</vuln:last-modified-datetime>
    <vuln:references xml:lang="en" reference_type="UNKNOWN">
      <vuln:source>MISC</vuln:source>
      <vuln:reference href="https://github.com/s3131212/allendisk/issues/16" xml:lang="en">https://github.com/s3131212/allendisk/issues/16</vuln:reference>
    </vuln:references>
    <vuln:summary>Allen Disk 1.6 has CSRF in setpass.php with an impact of changing a password.</vuln:summary>
